<div class="container">
  <div class="form-custom">
<%= form_for @attraction do |f| %>
  <div class="form-group">
    <%= f.label :attraction_name %>
    <%= f.text_field :attraction_name %>
  </div>

  <div class="form-group">
      <%= f.label :place_id, 'Places' %>
      <%= f.collection_select :place_id, Place.all, :id, :name, class: "form-control input-sm" %>
    </div>  
 

  <div class="form-group">
    <%= f.submit %> 
  </div>          
<% end %>
</div>
</div>