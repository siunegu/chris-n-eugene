<% if user_signed_in? && current_user.admin? %>
<%= link_to 'Add Attraction', new_attraction_path %>
<% end %>
<p>
<% @attraction.each do |attraction|  %>
<div id="list_items">
<%= link_to attraction.attraction_name, attraction  %>

<% if user_signed_in? && current_user.admin? %>
	<%= link_to 'Edit', edit_attraction_path(attraction) %>
	<%= link_to 'Destroy', attraction, method: :delete %>
<% end %>	

</div>
<% end %>
</p>