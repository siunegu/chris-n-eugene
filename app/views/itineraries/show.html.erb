<div class="container">
<h2><%= @itinerary.name %></h2>

<% @itinerary.places.each do |place| %>
  <%= place.name %>
<% end %>

<hr>
    <div class="row">
      <div class="photo-wrapper">
        <div class="photos">
          <% @photos.each do |photo| %>
          <div class="img">
            <%= image_tag(photo) %>
            <div class="overlay">
              <%= link_to "+", root_path, class: "expand" %>
              <a class="close-overlay hidden">x</a>
            </div>
          </div>
          <% end %>
        </div>
      </div>
    </div>
    </div>